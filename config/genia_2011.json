{
    "bert_model_name": "dmis-lab/biobert-large-cased-v1.1",
    "entity_span_len": 10,
    "trigger_span_len": 8,
    "max_entity_len": 22,
    "max_trigger_len": 9,
    "span_feature_dim": 256,
    "type_feature_dim": 256,
    "span_hidden_dim": 600,
    "span_dropout": 0.3,
    "bert_cache_dir": "bert",
    "multi_piece_strategy": "average",
    "bert_dropout": 0.5,
    "use_extra_bert": false,
    "extra_bert": -3,
    "complex_trigger_list": ["Positive_regulation", "Negative_regulation", "Regulation"],
    "multiple_trigger_list": ["Binding"],
  
    "use_global_features": false,
    "global_features": [],
    "global_warmup": 0,
  
    "linear_dropout": 0.4,
    "linear_bias": true,
    "bert_dim": 1024,
    "entity_hidden_num": 150,
    "mention_hidden_num": 150,
    "event_hidden_num": 600,
    "relation_hidden_num": 150,
    "role_hidden_num": 600,
    "use_entity_type": false,
    "edge_type_num": 13,
    "edge_type_dim": 256,
    "beam_size": 20,
    "lamda": 0.05,
    "beta_v": 2,
    "beta_e": 2,
