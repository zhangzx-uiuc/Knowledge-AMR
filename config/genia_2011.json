    "gnn_layers": 2,
  
    "accumulate_step": 1,
    "batch_size": 16,
    "eval_batch_size": 16,
    "max_epoch": 100,
    "learning_rate": 1e-3,
    "bert_learning_rate": 1e-5,
    "weight_decay": 1e-3,
    "bert_weight_decay": 1e-5,
    "warmup_epoch": 5,
    "grad_clipping": 5.0,
  
    "use_gpu": false,
    "gpu_device": 0
